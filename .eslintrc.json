{
	"parser": "@typescript-eslint/parser",
	"env": {
		"browser": true,
		"amd": true,
		"es6": true
	},
	"extends": [
		"plugin:react/recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier"
	],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"@typescript-eslint/explicit-function-return-type": "off"
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
